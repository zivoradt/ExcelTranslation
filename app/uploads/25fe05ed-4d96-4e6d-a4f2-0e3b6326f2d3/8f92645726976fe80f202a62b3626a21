    var _gsScope = 'undefined' != typeof module && module.exports && 'undefined' != typeof global ? global : this || window;
    (_gsScope._gsQueue || (_gsScope._gsQueue = [])).push(function () {
      'use strict';
      var t, e, i, s, r, n, a, o, l, h, _, u, f, c;
      _gsScope._gsDefine(
        'TweenMax',
        ['core.Animation', 'core.SimpleTimeline', 'TweenLite'],
        function (t, e, i) {
          var s = function (t) {
            var e,
              i = [],
              s = t.length;
            for (e = 0; e !== s; i.push(t[e++]));
            return i;
          },
            r = function (t, e, i) {
              var s,
                r,
                n = t.cycle;
              for (s in n) (r = n[s]), (t[s] = 'function' == typeof r ? r(i, e[i], e) : r[i % r.length]);
              delete t.cycle;
            },
            n = function (t) {
              if ('function' == typeof t) return t;
              var e = 'object' == typeof t ? t : { each: t },
                i = e.ease,
                s = e.from || 0,
                r = e.base || 0,
                n = {},
                a = isNaN(s),
                o = e.axis,
                l = { center: 0.5, end: 1 }[s] || 0;
              return function (t, h, _) {
                var u,
                  f,
                  c,
                  p,
                  m,
                  d,
                  g,
                  y,
                  v,
                  T = (_ || e).length,
                  x = n[T];
                if (!x) {
                  if (!(v = 'auto' === e.grid ? 0 : (e.grid || [1 / 0])[0])) {
                    for (g = -1 / 0; g < (g = _[v++].getBoundingClientRect().left) && v < T;);
                    v--;
                  }
                  for (x = n[T] = [], u = a ? Math.min(v, T) * l - 0.5 : s % v, f = a ? (T * l) / v - 0.5 : (s / v) | 0, g = 0, y = 1 / 0, d = 0; d < T; d++)
                    (c = (d % v) - u), (p = f - ((d / v) | 0)), (x[d] = m = o ? Math.abs('y' === o ? p : c) : Math.sqrt(c * c + p * p)), m > g && (g = m), m < y && (y = m);
                  (x.max = g - y), (x.min = y), (x.v = T = e.amount || e.each * (v > T ? T - 1 : o ? ('y' === o ? T / v : v) : Math.max(v, T / v)) || 0), (x.b = T < 0 ? r - T : r);
                }
                return (T = (x[t] - x.min) / x.max), x.b + (i ? i.getRatio(T) : T) * x.v;
              };
            },
            a = function (t, e, s) {
              i.call(this, t, e, s),
                (this._cycle = 0),
                (this._yoyo = !0 === this.vars.yoyo || !!this.vars.yoyoEase),
                (this._repeat = this.vars.repeat || 0),
                (this._repeatDelay = this.vars.repeatDelay || 0),
                this._repeat && this._uncache(!0),
                (this.render = a.prototype.render);
            },
            o = i._internals,
            l = o.isSelector,
            h = o.isArray,
            _ = (a.prototype = i.to({}, 0.1, {})),
            u = [];
          (a.version = '2.1.3'),
            (_.constructor = a),
            (_.kill()._gc = !1),
            (a.killTweensOf = a.killDelayedCallsTo = i.killTweensOf),
            (a.getTweensOf = i.getTweensOf),
