    <!-- S-pen -->
    <div id="s-pen" class="highlights-s-pen" bis_skin_checked="1">
      <div class="common-inner common-textbox-wrap--column" bis_skin_checked="1">
        <h2 class="common-sub-headline common-display-mo">
          <span class="common-text">
