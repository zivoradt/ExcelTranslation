<script src="/samsung/resources/global/galaxy-s24-ultra/common/js/common.min.js"></script>
<script>
  var LOCAL_VARI = {
    selected: "Selected",
    slide: "Slide",
    play: "play",
    pause: "pause",
    exclusive: "online exclusive colour",
    default: "default colour",
  };
</script>
<script id="viewer-js">
  var V3DLOCALDATA = {
    type: "V3DLOCALDATA",
    "galaxy-s24-ultra": {
      color: [
        "titanium-gray",
        "titanium-black",
        "titanium-violet",
        "titanium-yellow",
        "titanium-blue",
        "titanium-green",
        "titanium-orange",
      ],
    },
    text: {
      "Titanium Gray(Galaxy S24 Ultra)": "Titanium Grey",
      "Titanium Black(Galaxy S24 Ultra)": "Titanium Black",
      "Titanium Violet(Galaxy S24 Ultra)": "Titanium Violet",
      "Titanium Yellow(Galaxy S24 Ultra)": "Titanium Yellow",
      "Titanium Blue(Galaxy S24 Ultra)": "Titanium Blue",
